{
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "target": "ESNext",
    "jsx": "preserve",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noFallthroughCasesInSwitch": true,
    "plugins": [{ "name": "@effect/language-service" }]
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
